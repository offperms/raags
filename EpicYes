if down == true then
    cumValue.Value = cumValue.Value +1
    down = false
   YvalO.Value = -5
   wait()
   YvalO.Value = -15
   wait()
   YvalO.Value = -25
   wait()
   YvalO.Value = -35
   wait()
   up = true
end
end)

while wait() do
   if cumValue.Value >= 100 then
       cum()
    end
end 
