if chr.Humanoid.Health == 0 then
	toggle = false
end
for _, v in pairs(hum:GetAccessories()) do
if v.Name == HAT then
local b = v.Handle;
    b.Orientation = Vector3.new(part.Orientation.X + XvalO.Value, part.Orientation.Y + YvalO.Value, part.Orientation.Z + ZvalO.Value)
