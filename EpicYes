	toggle = false
end
for _, v in pairs(hum:GetAccessories()) do
if v.Name == HAT then
local b = v.Handle;
    b.Orientation = Vector3.new(plr.Character.Head.Orientation.X + XvalO.Value, plr.Character.Head.Orientation.Y + YvalO.Value, plr.Character.Head.Orientation.Z + ZvalO.Value)
