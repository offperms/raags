    if up == true then
    cumValue.Value = cumValue.Value +1
    up = false
    YvalO.Value = -45
   wait()
   YvalO.Value = -35
   wait()
   YvalO.Value = -25
   wait()
   YvalO.Value = -15
   wait()
   YvalO.Value = -5
   wait()
   YvalO.Value = 0
   down = true
    end
end)
