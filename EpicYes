plr.Head.Mesh:Destroy()
game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
wait(2)
local function die()
game:GetService('RunService').Stepped:Connect(function()
    game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
    game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
        game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
    game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
        game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
    game.ReplicatedStorage.RemoteFunctions.NameChangerRequest:InvokeServer("e", "eee")
end)
end
game:GetService('RunService').Stepped:Connect(function()
die()
die()
die()
die()
die()
die()
end)
end)

